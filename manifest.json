{
  "manifest_version": 3,
  "name": "Tab Cleanup",
  "version": "1.0.0",
  "description": "Clean up inactive tabs automatically. Set custom time ranges to keep your browser organized and efficient.",
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "permissions": [
    "tabs",
    "storage",
    "scripting",
    "activeTab"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "32": "icons/icon32.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  },
  "author": "Bryant Hao",
  "homepage_url": "https://github.com/bryant24hao/tab-cleanup"
}